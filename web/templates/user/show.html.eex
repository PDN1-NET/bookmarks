<h2>Show user</h2>

<ul>

  <li>
    <strong>Nick Name:</strong>
    <%= @user.username %>
  </li>

  <li>
    <strong>Full Name:</strong>
    <%= @user.fullname %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

</ul>

<%= link "Edit", to: user_path(@conn, :edit, @user) %>
<%= link "Back", to: user_path(@conn, :index) %>
